

<h2>Claim Status</h2>

<table class="table">
    <thead>
        <tr>
            <th>Claim ID</th>
            <th>Hours Worked</th>
            <th>Hourly Rate</th>
            <th>Status</th>
            <th>Submitted Date</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var claim in Model)
        {
            <tr>
                <td>@claim.ClaimID</td>
                <td>@claim.HoursWorked</td>
                <td>@claim.HourlyRate</td>
                <td>@claim.Status</td>
                <td>@claim.SubmittedDate</td>
            </tr>
        }
    </tbody>
</table>
